{
  "name": "bullmq-example",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc ./src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^18.15.10",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "bullmq": "^3.10.2",
    "ioredis": "^5.3.1"
  }
}
